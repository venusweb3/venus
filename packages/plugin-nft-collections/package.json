{
    "name": "@venusos/plugin-nft-collections",
    "version": "0.1.0",
    "description": "NFT collections plugin for venus",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint --fix --cache .",
        "format": "prettier --write src/**/*.ts"
    },
    "dependencies": {
        "@venusos/core": "workspace:*",
        "@venusos/plugin-evm": "workspace:*",
        "axios": "^1.6.7",
        "rate-limiter-flexible": "^5.0.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^9.16.0",
        "prettier": "^3.2.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^2.1.5"
    },
    "peerDependencies": {
        "@venusos/core": "workspace:*"
    }
}
